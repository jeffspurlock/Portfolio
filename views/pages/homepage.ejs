<script type="text/javascript">
    window.onload = ()=>{ 
      disableScroll();
      fadeOut(document.getElementById("cover"), 2000, 1);
      setTimeout(()=>{ document.getElementById("cover").style.display = "none"}, 3000);
      setTimeout(enableScroll(), 3000);
      var package = getObjectsDesktop();
      document.addEventListener('wheel', animate.bind(null,event,package));
     }
    
   
    
    

</script> 
