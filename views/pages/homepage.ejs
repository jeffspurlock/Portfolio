<script type="text/javascript">
    window.onload = ()=>{ 
      disableScroll();
      setTimeout(enableScroll(), 3000);
      if(window.innerWidth > 800){
        var layout="desktop";
      } else {
        var layout="mobile";
      };
        var positionPackage = getObjects(layout);
          document.addEventListener('wheel', animate.bind(null,event,positionPackage));
          document.addEventListener("touchmove", animate.bind(null, event, positionPackage));
      }
    
</script> 
