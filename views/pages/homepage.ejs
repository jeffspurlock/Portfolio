<script type="text/javascript">
    window.onload = ()=>{ 
      disableScroll();
      setTimeout(enableScroll(), 3000);
      if($(window).width > 800){
        layout="desktop"
      } else {
        layout="mobile"
      }
        var package = getObjects(layout);
        document.addEventListener('wheel', animate.bind(null,event,package));
        document.addEventListener("touchmove", animate.bind(null, event, package));
      }
    
</script> 
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
